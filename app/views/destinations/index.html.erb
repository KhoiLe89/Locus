<div class="<%=@continent.name%>1">
  <h1> All Destinations in <%=@continent.name%> </h1>
<ul>
<% @destinations.each do |destination| %>
<li><%= link_to destination.location, destination_activities_path(destination) %></li>
<% end %>
</ul>
<%= link_to "Add a new Destination", new_continent_destination_path(@continent) %>
</div>
